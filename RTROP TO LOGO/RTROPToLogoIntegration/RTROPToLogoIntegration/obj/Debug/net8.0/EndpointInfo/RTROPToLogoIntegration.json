{
  "openapi": "3.0.1",
  "info": {
    "title": "Logo MRP API",
    "version": "v1"
  },
  "paths": {
    "/api/Auth/login": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Mrp/calculate-json": {
      "post": {
        "tags": [
          "Mrp"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/MrpRawItemDto"
                }
              }
            },
            "text/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/MrpRawItemDto"
                }
              }
            },
            "application/*+json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/MrpRawItemDto"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Mrp/calculate-excel": {
      "post": {
        "tags": [
          "Mrp"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "file": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Stock/status/{itemRef}": {
      "get": {
        "tags": [
          "Stock"
        ],
        "parameters": [
          {
            "name": "itemRef",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LoginDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "MrpRawItemDto": {
        "type": "object",
        "properties": {
          "itemID": {
            "type": "string",
            "nullable": true
          },
          "roP_update_ABCDClassification": {
            "type": "string",
            "nullable": true
          },
          "planningType": {
            "type": "string",
            "nullable": true
          },
          "safetyStock": {
            "type": "number",
            "format": "double"
          },
          "rop": {
            "type": "number",
            "format": "double"
          },
          "max": {
            "type": "number",
            "format": "double"
          },
          "roP_update_OrderQuantity": {
            "type": "number",
            "format": "double"
          },
          "itemRef": {
            "type": "integer",
            "format": "int32"
          },
          "bomMasterRef": {
            "type": "integer",
            "format": "int32"
          },
          "bomRevRef": {
            "type": "integer",
            "format": "int32"
          },
          "clientRef": {
            "type": "integer",
            "format": "int32"
          },
          "itemType": {
            "type": "string",
            "nullable": true
          },
          "ambar": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "JWT Authorization header using the Bearer scheme. Example: \"Authorization: Bearer {token}\"",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
}